{
  "name": "audigo",
  "version": "0.1.0",
  "description": "Simple audio recording + upload + sharing web app",
  "private": true,
  "main": "dist/server",
  "author": "Devin Spikowski",
  "license": "MIT",
  "homepage": "https://github.com/vegeta897/audigo",
  "bugs": {
    "url": "https://github.com/vegeta897/audigo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vegeta897/audigo.git"
  },
  "scripts": {
    "start": "node .",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "lint": "eslint src private webpack.config.js",
    "storybook": "start-storybook -p 9002 -c private/storybook",
    "predev": "npm run prebuild",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack"
  },
  "jest": {
    "collectCoverage": true,
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^(components|containers)$": "<rootDir>/private/jest/componentsMock.js",
      "^(store|\\.\\.)\\/selectors$": "<rootDir>/private/jest/selectorsMock.js",
      "^(store|\\.\\.)\\/actions$": "<rootDir>/private/jest/actionsMock.js"
    },
    "setupFiles": [
      "<rootDir>/private/jest/setupTests.js"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.0",
    "delay": "^2.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^1.1.5",
    "jest-cli": "^22.1.4",
    "lodash-webpack-plugin": "^0.11.4",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-assets-by-type-plugin": "^0.1.0",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-server-source-map": "^0.2.0",
    "webpack-blocks-split-vendor": "^0.2.1",
    "webpack-child-config-plugin": "^0.1.2",
    "webpack-dev-server": "^2.3.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-spawn-plugin": "^0.1.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "brute-knex": "^2.1.0",
    "express": "^4.16.2",
    "express-brute": "^1.0.1",
    "fs-extra": "^5.0.0",
    "helmet": "^3.10.0",
    "history": "^4.6.1",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.14.2",
    "lodash": "^4.17.4",
    "normalizr": "3.2.4",
    "pg": "^7.4.1",
    "prop-types": "^15.5.8",
    "query-string": "^5.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.1.1",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-server": "^4.2.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.0",
    "redux-saga": "^0.16.0",
    "redux-saga-thunk": "^0.5.0",
    "serialize-javascript": "^1.3.0",
    "shortid": "^2.2.8",
    "simple-load-script": "^1.0.2",
    "slug": "^0.9.1",
    "styled-components": "^3.0.2",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.4.0",
    "validator": "^9.0.0"
  }
}
